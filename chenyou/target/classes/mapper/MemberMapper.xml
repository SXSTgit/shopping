<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.itsq.mapper.MemberMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.itsq.pojo.entity.Member">
        <id column="id" property="id" />
        <result column="manager_id" property="managerId" />
        <result column="name" property="name" />
        <result column="phone" property="phone" />
        <result column="amount" property="amount" />
        <result column="cre_date" property="creDate" />
        <result column="up_date" property="upDate" />
        <result column="state" property="state" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, manager_id, name, phone, amount, cre_date, up_date, state
    </sql>

</mapper>
